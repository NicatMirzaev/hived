{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "no-console":"off",
        "indent": ["error", 2],
        "no-shadow":"off",
        "no-promise-executor-return": 0,
        "global-require":"off"
    }
}
